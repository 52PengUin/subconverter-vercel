{
    "name": "subconverter-ZEIT",
    "version": 2,
    "builds": [
        {
            "src": "bootstrap.sh", "use": "@now/bash"
        },
        {
            "src": "subconverter/subconverter.exe",
            "use": "now-static-bin",
            "config": {
                "port": 25500,
                "timeout": 1500,
                "includeFiles": [
                    "subconverter/**"
                ]
            }
        }
    ],
    "routes": [
        {"src": "/(.*)", "dest": "subconverter/subconverter.exe"}
    ]
}
